<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:name="uz.gita.quizeappmvpmehriddins.app.App"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizeAppMVP"
        tools:targetApi="31">
        <activity
            android:name="uz.gita.quizeappmvpmehriddins.ui.result.ResultActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="uz.gita.quizeappmvpmehriddins.ui.game.GameActivity"
            android:screenOrientation="portrait"
            android:exported="false"/>
        <activity
            android:name="uz.gita.quizeappmvpmehriddins.ui.choose.ChooseActivity"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="uz.gita.quizeappmvpmehriddins.ui.info.InfoActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
    </application>

</manifest>